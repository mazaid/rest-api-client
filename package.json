{
  "name": "mazaid-rest-api-clients",
  "version": "0.1.8",
  "description": "rest api clients",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf package",
    "build": "node ./build.js",
    "docs:open": "npm run docs && open ./esdoc/index.html",
    "docs": "rm -rf ./esdoc && ./node_modules/.bin/esdoc ./ -c ./.esdoc.json",
    "tests": "./node_modules/.bin/mocha ./test/",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha ./test/",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js src",
    "lint:fix": "./node_modules/.bin/eslint -c .eslintrc.js --fix src"
  },
  "engines": {
    "node": ">=4"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mazaid/rest-checks.git"
  },
  "keywords": [
    "mazaid"
  ],
  "author": "alekzonder <alekzonder@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mazaid/rest-checks/issues"
  },
  "homepage": "https://github.com/mazaid/rest-checks#readme",
  "dependencies": {
    "mazaid-error": "^0.3.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-json-schema": "^1.4.0",
    "codeclimate-test-reporter": "^0.3.3",
    "esdoc": "^0.4.8",
    "esdoc-node": "^1.0.0",
    "eslint": "^3.11.1",
    "fs-extra": "^0.30.0",
    "ink-docstrap": "^1.2.1",
    "istanbul": "^0.4.4",
    "jsdoc": "^3.4.0",
    "log4js": "^0.6.38",
    "minami": "^1.1.1",
    "mocha": "^2.4.5"
  }
}
